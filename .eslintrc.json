{
  "extends": [
    "@rocketseat/eslint-config/node"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "tabWidth": 2,
        "singleQuote": true,
        "endOfLine": "auto",
        "arrowParens": "always",
        "semi": false,
        "trailingComma": "all"
      }
    ]
  }
}